{
  "name": "secretary",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format:fix": "prettier --write --ignore-path .gitignore './**/*.{js,ts,tsx}'",
    "format:check": "prettier --check --ignore-path .gitignore './**/*.{js,ts,tsx}'",
